    "@typescript-eslint/parser": "^5.38.1",
    "babel-jest": "^27.1.0",
    "eslint": "^8.24.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "husky": ">=6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.0",
    "lint-staged": ">=10",
    "prettier": "^2.7.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn lint:eslint",
      "yarn test",
      "yarn format"
