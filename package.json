    "@radix-ui/react-tabs": "^1.0.0",
    "@stitches/react": "^1.2.8",
    "framer-motion": "^4.1.17",
    "next": "^12.3.1",
    "next-mdx-remote": "^3.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.14.2",
    "react-intersection-observer": "^8.32.0",
    "rehype-sanitize": "^5.0.0",
    "rehype-slug": "^5.0.0",
